  <!-- Scripts -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.5/dist/js/bootstrap.bundle.min.js"></script>
  <script>
    document.addEventListener('DOMContentLoaded', function() {
      // Animation on scroll
      const fadeElems = document.querySelectorAll('.fadeIn');
      fadeElems.forEach(el => {
        el.style.opacity = '1';
        el.style.transform = 'translateY(0)';
      });
      
      // Theme toggler
      const themeTogglers = document.querySelectorAll('.theme-item');
      themeTogglers.forEach(toggler => {
        toggler.addEventListener('click', () => {
          const theme = toggler.getAttribute('data-bs-theme-value');
          document.documentElement.setAttribute('data-bs-theme', theme);
          
          // Update active class
          themeTogglers.forEach(t => t.classList.remove('active'));
          toggler.classList.add('active');
          
          // Update icon
          const icon = document.querySelector('#themeToggle i');
          if (theme === 'light') {
            icon.className = 'bi bi-sun-fill';
          } else if (theme === 'dark') {
            icon.className = 'bi bi-moon-stars-fill';
          } else {
            icon.className = 'bi bi-circle-half';
          }
        });
      });
      
      // Seat selection functionality
      const seats = document.querySelectorAll('.seat:not(.occupied)');
      const selectedSeatsContainer = document.getElementById('selectedSeatsContainer');
      const continueBtn = document.getElementById('continueBtn');
      const totalPriceElement = document.querySelector('.total-price-value');
      
      let selectedSeats = [];
      const regularPrice = 1500;
      const premiumPrice = 2000;
      
      seats.forEach(seat => {
        seat.addEventListener('click', () => {
          seat.classList.toggle('selected');
          
          const row = seat.getAttribute('data-row');
          const seatNum = seat.getAttribute('data-seat');
          const isPremium = seat.classList.contains('premium');
          const seatInfo = {
            row: row,
            seat: seatNum,
            premium: isPremium,
            price: isPremium ? premiumPrice : regularPrice
          };
          
          if (seat.classList.contains('selected')) {
            selectedSeats.push(seatInfo);
          } else {
            selectedSeats = selectedSeats.filter(s => !(s.row === row && s.seat === seatNum));
          }
          
          updateSelectedSeatsUI();
        });
      });
      
      function updateSelectedSeatsUI() {
        if (selectedSeats.length === 0) {
          selectedSeatsContainer.innerHTML = '<p class="text-center text-muted">No seats selected yet</p>';
          continueBtn.disabled = true;
        } else {
          selectedSeatsContainer.innerHTML = '<ul class="selected-seats"></ul>';
          const selectedSeatsList = selectedSeatsContainer.querySelector('.selected-seats');
          
          let totalPrice = 0;
          
          selectedSeats.forEach(seat => {
            const li = document.createElement('li');
            li.className = 'selected-seat-item';
            
            const seatLabel = document.createElement('span');
            seatLabel.textContent = `${seat.row}${seat.seat} ${seat.premium ? '(Premium)' : ''}`;
            
            const seatPrice = document.createElement('span');
            seatPrice.className = 'seat-price';
            seatPrice.textContent = `PKR ${seat.price}`;
            
            li.appendChild(seatLabel);
            li.appendChild(seatPrice);
            selectedSeatsList.appendChild(li);
            
            totalPrice += seat.price;
          });
          
          totalPriceElement.textContent = `PKR ${totalPrice}`;
          continueBtn.disabled = false;
        }
      }
      
      // Continue button functionality
      continueBtn.addEventListener('click', () => {
        alert('Redirecting to snacks selection...');
        // In a real application, you would redirect or proceed to the next step
        // window.location.href = '/snacks';
      });
    });
  </script>
