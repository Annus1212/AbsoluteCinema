            <i class="bi bi-search search-icon"></i>
            <input id="searchInput" type="search" class="form-control" placeholder="Search movies..." aria-label="Search" autocomplete="off" style="width: 300px;position: absolute;top: 20px;right:180px">
            </div>
        </div>

        <div class="d-flex flex-wrap align-items-center mt-3">
